{
  "expo": {
    "name": "FMT News",
    "slug": "FMT-News-App",
    "version": "2.1.5",
    "icon": "./app/assets/images/ic_launcherA.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./app/assets/images/ic_launcherA.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "icon": "./app/assets/images/fmtlogo.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.freemalaysiatoday.app.fmtnews.ios",
      "googleServicesFile": "./GoogleService-Info.plist",
      "jsEngine": "hermes",
      "name": "FMT News",
      "splash": {
        "backgroundColor": "#ffffff"
      }
    },
    "android": {
      "icon": "./app/assets/images/ic_launcherA.png",
      "enableDangerousExperimentalLeanBuilds": false,
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/ic_launcherA.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.freemalaysiatoday.app.fmtnews.android",
      "label": "FMT News",
      "permissions": ["POST_NOTIFICATIONS"],
      "jsEngine": "hermes"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./app/assets/images/ic_launcherA.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-1691191334203390~5037677560",
          "android_app_id": "ca-app-pub-1691191334203390~5037677560",
          "iosAppId": "ca-app-pub-1691191334203390~9795180220"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./app/assets/images/iconFMT.jpg",
          "color": "#ffffff",
          "androidMode": "default",
          "androidCollapsedTitle": "New messages"
        }
      ]
    ],
    "experiments": { "typedRoutes": true },
    "extra": {
      "eas": { "projectId": "77fd98c7-01bb-4b3c-963e-4815cea0996c" },
      "router": { "origin": false }
    },
    "owner": "mohiteas"
  }
}
